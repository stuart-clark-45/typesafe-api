{
  "name": "typesafe-api",
  "version": "0.3.1",
  "description": "A TypeScript library that can be used to rapidly create typesafe APIs, API Clients and express routes + controllers",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "source": "src/index.ts",
  "repository": "https://github.com/stuart-clark-45/typesafe-api",
  "homepage": "https://github.com/stuart-clark-45/typesafe-api/blob/master/README.md",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rm -rf dist && tsc",
    "start": "tsc -w",
    "lint": "eslint './src/**/*.ts'",
    "prepare": "npm run build",
    "preversion": "npm run lint && npm run build",
    "test": "jest -i --coverage",
    "docs": "embedme README.md"
  },
  "keywords": [
    "typesafe",
    "api",
    "type safe",
    "client",
    "generator"
  ],
  "author": "victorlcm",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "@types/node": "14.17.1",
    "@typescript-eslint/eslint-plugin": "4.26.0",
    "@typescript-eslint/parser": "4.26.0",
    "bson-objectid": "^2.0.1",
    "embedme": "^1.22.0",
    "eslint": "7.27.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "3.4.0",
    "find-free-ports": "^3.0.0",
    "jest": "^27.1.0",
    "prettier": "2.3.0",
    "ts-jest": "^27.0.5",
    "typescript": "4.3.2"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "deepmerge": "^4.2.2",
    "express": "^4.17.1",
    "tslib": "^2.0.1",
    "url-join-ts": "^1.0.5",
    "@types/express": "^4.17.13"
  }
}
